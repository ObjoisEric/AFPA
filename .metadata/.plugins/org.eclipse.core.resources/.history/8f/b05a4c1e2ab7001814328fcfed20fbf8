
public class OEExoWhileBoucle {

	public static void main(String[] args) {
		int b = 1;
		
		
		while ( b <= 100) {
			
			System.out.println("b : " +b);
			if ( b < 100) {
				b++;
			}
			
		}
		
		System.out.println("intermission b : " +b);


		while ( b >= 1) {
		
			System.out.println("b desc : " +b);
			b--;
			
			
		}
		
		
		
	}

}
