import java.util.Scanner;

public class ExoVariables {
	
	public static void main(String[] args) {
	
		
		//Declaration variable
		
		
		
		
		
		
		
	
		Scanner sc = new Scanner(System.in);
		
	
	
	float note = sc.nextFloat();
		int coef = sc.nextInt();
		float resultat;
	//calcul
		resultat = (note * coef);
	
	//affichage
		
		
		System.out.println("note * coef = " + resultat);
	
	
	
	
	
	
	
	
	
	
	
	}
}
