
public class ExoConditionIfElse {

	public static void main(String[] args) {

		String connexion = "connecté";

		if (connexion == "connecté") {

			System.out.println("Vous êtes connecté");

		}
		else {

			System.out.println("Vous n'êtes pas connecté");

		}

		char caracter ='a';
		
		
		if (caracter == 'a') {

			System.out.println("Votre caractère est égale à a");

		}
		else {

			System.out.println("Votre caracète n'est pas égale à a");

		}		
		
		
		
		
		
		
	}

}
